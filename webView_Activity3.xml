<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/button_plan1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:text="Workout plan 1"
        app:layout_constraintEnd_toStartOf="@+id/button_plan2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_plan2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="5dp"
        android:text="Workout plan 4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_plan1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_plan3"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Workout plan 2"
        app:layout_constraintEnd_toStartOf="@+id/button_plan4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_plan1" />

    <Button
        android:id="@+id/button_plan4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:text="Workout plan 5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_plan3"
        app:layout_constraintTop_toBottomOf="@+id/button_plan2" />

    <Button
        android:id="@+id/button_plan5"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Workout plan 3"
        app:layout_constraintEnd_toStartOf="@+id/button_plan6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_plan3" />

    <Button
        android:id="@+id/button_plan6"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:text="Workout plan 6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_plan5"
        app:layout_constraintTop_toBottomOf="@+id/button_plan4" />

    <Button
        android:id="@+id/button_meal1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="19dp"
        android:text="Meal plan 1"
        app:layout_constraintEnd_toStartOf="@+id/button_meal2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_plan5" />

    <Button
        android:id="@+id/button_meal2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="5dp"
        android:text="meal plan 4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_meal1"
        app:layout_constraintTop_toBottomOf="@+id/button_plan6" />

    <Button
        android:id="@+id/button_meal3"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="meal plan 2"
        app:layout_constraintEnd_toStartOf="@+id/button_meal4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_meal1" />

    <Button
        android:id="@+id/button_meal4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:text="meal plan 5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_meal3"
        app:layout_constraintTop_toBottomOf="@+id/button_meal2" />

    <Button
        android:id="@+id/button_meal5"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="meal plan 3"
        app:layout_constraintEnd_toStartOf="@+id/button_meal6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_meal3" />

    <Button
        android:id="@+id/button_meal6"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="7dp"
        android:text="meal plan 6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_meal5"
        app:layout_constraintTop_toBottomOf="@+id/button_meal4" />

    <WebView
        android:id="@+id/webView_main"
        android:layout_width="415dp"
        android:layout_height="333dp"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="397dp" >

    </WebView>

</androidx.constraintlayout.widget.ConstraintLayout>



